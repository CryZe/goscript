package main

func f1() {
    var j int
    var s = map[int]int{123:10, 258:20}
    var s2 = map[int]int{1:100, 2:200}
    for k, v := range s {
        j += v
        for k2, v2 := range s2{
            j += v2
            for k, v := range s {
                j += v
            }
        }
    }
    assert(j == 750)
}


func f2() {
    j := 0
    for i := 0; i < 1000; i++ {
        j += 1
    }
    assert(j == 1000)
}

func f3() {
    i := 0
    i += 'a'
    i += 'b'
    i += 'c'
    i += 'd'
    var j, k, j2, k2 int
    for _, c := range "abcd" {
        j += c
    }
    for i := range "abcd" {
        k += i
    }
    for i, c := range "abcd" {
        j2 += c
        k2 += i
    }

    assert(i == j)
    assert(6 == k)
    assert(j2 == j)
    assert(k2 == k)
}



func main() {
   f1()
   f2()
   f3()

