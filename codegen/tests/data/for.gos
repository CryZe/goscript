package main

func f1() {
    var j int
    var s = map[int]int{123:10, 258:20}
    var s2 = map[int]int{1:100, 2:200}
    for _, v := range s {
        j += v
        for _, v2 := range s2{
            j += v2
            for _, v := range s {
                j += v
            }
        }
    }
    assert(j == 750)
}


func f2() {
    j := 0
    for i := 0; i < 1000; i++ {
        j += 1
    }
    assert(j == 1000)
}

